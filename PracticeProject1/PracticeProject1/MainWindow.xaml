<Window x:Class="PracticeProject1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticeProject1"
        mc:Ignorable="d"
        Title="MainWindow" Height="232.512" Width="392.53">
    <Grid Margin="0,0,3,0.667">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="okbtn" Content="Open" HorizontalAlignment="Left" Margin="55,125,0,0" VerticalAlignment="Top" Width="80" Click="Button_Click" Grid.Column="1" Height="20"/>
        <Button x:Name="cnclbtn" Content="Cancel" HorizontalAlignment="Left" Margin="250,125,0,0" VerticalAlignment="Top" Width="80" Grid.Column="1" Height="20" Click="cnclbtn_Click"/>
        <Label x:Name="errmsg" Content="Click to open the Message. open message, again" Margin="55,50,0,0" VerticalAlignment="Top" Grid.Column="1" Height="26" HorizontalAlignment="Left" Width="275"/>

    </Grid>
</Window>
